{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "cors": {
    "allowedOrigins": [
      "https://localhost:5001",
      "https://localhost:5001/",
      "http://localhost:5002",
      "https://localhost:7071"
    ],
    "supportCredentials": false
  }
}
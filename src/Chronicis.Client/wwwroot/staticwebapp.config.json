{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/js/*", "/images/*", "/_framework/*", "/_content/*", "/api/*", "*.ico", "*.png", "*.jpg", "*.svg"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/authentication/login"
    }
  }
}
